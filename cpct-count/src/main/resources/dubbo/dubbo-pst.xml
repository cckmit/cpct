<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://code.alibabatech.com/schema/dubbo
    http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <!-- 提供方应用信息，用于计算依赖关系 33.1059  00.1083-->
    <dubbo:application name="00.1083"/>

    <dubbo:registry id="esb3" protocol="dubbo" address="134.96.253.65:8001"/>
    <dubbo:registry id="esb2" protocol="dubbo" address="134.96.159.11:8033"/>

    <dubbo:protocol name="dubbo" port="30803"/>
    <dubbo:monitor protocol="registry" />


    <!--客户分群id标签试算-->
    <dubbo:service registry="esb2" interface="com.zjtelcom.cpct.count.service.api.GroupApiService" ref="groupApiServiceImpl" version="1.0" >
        <dubbo:method name="groupTrial" servicecode="00.1083.groupTrial"/>
    </dubbo:service>

    <!-- es提供试算接口 -->
    <dubbo:reference registry="esb2" interface="com.zjtelcom.es.es.service.EsService" id="esService"
                     version="1.0" retries="0" check="false">
        <dubbo:method name="searchByTarGrp" servicecode="00.1083.searchByTarGrp" timeout="10000"/>
    </dubbo:reference>



</beans>
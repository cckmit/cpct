<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://dubbo.apache.org/schema/dubbo" xmlns="http://www.springframework.org/schema/beans"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd
       http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd
        http://dubbo.apache.org/schema/dubbo http://dubbo.apache.org/schema/dubbo/dubbo.xsd">

    <!-- 提供方应用信息，用于计算依赖关系 33.1059  00.1083-->
    <dubbo:application name="00.1083"/>

    <dubbo:registry id="esb3" protocol="dubbo" address="134.96.253.65:8001"/><!--bss3.0 pst环境-->
    <dubbo:registry id="esb2" protocol="dubbo" address="134.96.161.20:8001"/><!--esb2.0生产环境-->
    <dubbo:registry id="esb4" protocol="dubbo" address="134.96.249.149:8001"/><!--bss3.0 prod环境-->

    <!-- 暴露服务 -->
    <dubbo:protocol name="dubbo" port="30809"/>
    <dubbo:monitor protocol="registry" />

    <!-- 因子实时查询 -->
    <dubbo:reference registry="esb4" interface="com.zjpii.biz.serv.YzServ" id="yzServ"
                     version="1.0" retries="0" check="false">
        <dubbo:method name="queryYz" servicecode="00.1083.queryYz" timeout="10000"/>
    </dubbo:reference>


    <!--crm资产查询服务-->
    <dubbo:reference registry="esb2" interface="com.zjpii.biz.service.crm.AssetQueryService" id="assetQueryService"
                     version="1.0" retries="0" check="false">
        <dubbo:method name="assetQueryList" servicecode="33.1016.assetQueryList"/>
    </dubbo:reference>

    <!--协同渠道接触任务-->
    <dubbo:reference registry="esb4" interface="com.ctzj.smt.bss.cooperate.service.dubbo.IContactTaskService" id="iContactTaskService"
                     version="1.0" retries="0" check="false">
        <dubbo:method name="queryTaskDetail4openApi" servicecode="00.1079.queryTaskDetail4openApi"/>
    </dubbo:reference>

    <!--协同通过号码查活动-->
    <dubbo:reference registry="esb4" interface="com.ctzj.smt.bss.cooperate.service.dubbo.IReportService" id="reportServiceImpl"
                     version="1.0" retries="0" check="false">
        <dubbo:method name="queryCampaignByNum" servicecode="00.1079.queryValidCampaignByAccNum"/>
    </dubbo:reference>

    <!-- 集团下发活动反馈接口 1.0 -->
    <dubbo:service registry="esb4" interface="com.zjtelcom.cpct.open.service.completeMktCampaign.OpenCompleteMktCampaignService"
                   ref="openCompleteMktCampaignServiceImpl" version="1.0" >
        <dubbo:method name="completeMktCampaign" servicecode="00.1083.completeMktCampaign" timeout="10000"/>
    </dubbo:service>

    <!-- UCCP短信发送 -->
    <dubbo:reference registry="esb3" interface="com.ztesoft.uccp.dubbo.interfaces.UCCPSendService" id="uCCPSendService" version="1.0" check="false">
        <dubbo:method name="sendShortMessage" servicecode="33.1099.sendShortMessage" timeout="120000"/>
    </dubbo:reference>
</beans>
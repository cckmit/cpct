<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://code.alibabatech.com/schema/dubbo
    http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <!-- 提供方应用信息，用于计算依赖关系 33.1059  00.1083-->
    <dubbo:application name="00.1083"/>

    <dubbo:registry id="esb3" protocol="dubbo" address="134.96.253.65:8001"/>
    <dubbo:registry id="esb2" protocol="dubbo" address="134.96.159.11:8033"/>

    <dubbo:registry id="esb1" protocol="dubbo" address="134.96.253.59:8033"/> <!--msit环境-->


    <!-- 因子实时查询 -->
    <dubbo:reference registry="esb3" interface="com.zjpii.biz.serv.YzServ" id="yzServ"
                     version="1.0" retries="0" check="false">
        <dubbo:method name="queryYz" servicecode="00.1083.queryYz" timeout="10000"/>
    </dubbo:reference>

    <!--CRM标识查询-->
    <dubbo:reference registry="esb1" interface="com.ctzj.smt.bss.custview.query.service.api.ICustomerDubboService" id="iCustomerDubboService"
                     version="1.0" retries="0" check="false">
        <dubbo:method name="getCrmPrivil" servicecode="33.1074.getCrmPrivil" timeout="30000"/>
    </dubbo:reference>

    <!--用户信息查询-->
    <dubbo:reference registry="esb1" interface="com.ctzj.smt.bss.custview.query.service.api.IUserDubboService"
                     id="iUserDubboService" version="1.0">
        <dubbo:method name="qryCustProdInst" servicecode="33.1074.qryCustProdInst"/>
    </dubbo:reference>

</beans>
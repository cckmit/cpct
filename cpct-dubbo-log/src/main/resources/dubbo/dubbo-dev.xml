<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://code.alibabatech.com/schema/dubbo
    http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <!-- 提供方应用信息，用于计算依赖关系 -->
    <dubbo:application name="00.1083"/>

    <dubbo:registry id="esb_dev" protocol="dubbo" address="134.96.253.86:8033"/>
    <dubbo:registry id="esb3" protocol="dubbo" address="134.96.253.65:8001"/>
    <dubbo:registry id="esb2" protocol="dubbo" address="134.96.159.11:8033"/>

    <!-- 暴露服务 -->
    <dubbo:protocol name="dubbo" port="30303"/>
    <dubbo:monitor protocol="registry" />

    <!-- eslog -->
    <dubbo:service registry="esb2" id="mqProducerService"  interface="com.zjtelcom.cpct.elastic.service.MqProducerService" ref="mqProducerServiceImpl" version="1.0" >
        <dubbo:method name="msg2ESLogProducer" servicecode="00.1083.msg2ESLogProducer"  timeout="10000"/>
    </dubbo:service>

    <dubbo:service registry="esb2" id="esHitService"  interface="com.zjtelcom.cpct.elastic.service.EsHitService" ref="esHitServiceImpl" version="1.0" >
        <dubbo:method name="add" servicecode="00.1083.add"  timeout="10000"/>
        <dubbo:method name="save" servicecode="00.1083.save"  timeout="10000"/>
        <dubbo:method name="searchCampaignHitsInfo" servicecode="00.1083.searchCampaignHitsInfo"  timeout="10000"/>
        <dubbo:method name="searchCampaignHitsTotal" servicecode="00.1083.searchCampaignHitsTotal"  timeout="10000"/>
        <dubbo:method name="searchLabelInfoByRuleId" servicecode="00.1083.searchLabelInfoByRuleId"  timeout="10000"/>
    </dubbo:service>

</beans>